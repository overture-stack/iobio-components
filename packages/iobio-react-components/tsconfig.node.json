{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"allowImportingTsExtensions": true,
		"composite": true,
		"module": "ESNext",
		"moduleResolution": "bundler",
		"noEmit": true,
		"paths": {
			"iobio-charts/*": ["./src/iobio.d.ts"],
			"vite-plugin-dts": ["./node_modules/vite-plugin-dts/dist/index.d.ts"]
		},
		"skipLibCheck": true,
		"strict": true,
		"target": "esnext",
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo"
	},
	"include": ["vite.config.ts", "src/utils/"]
}
